{
  "name": "ondetempico",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "storybook": "storybook start"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFiles": [
      "./src/jestSetup.js"
    ]
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "expo": "^32.0.4",
    "firebase": "^5.5.8",
    "formik": "^1.3.2",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-fbsdk": "^0.8.0",
    "react-native-maps": "^0.22.1",
    "react-navigation": "^2.18.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/expo": "^30.0.0",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.7.4",
    "@types/react-native": "^0.57.8",
    "@types/react-navigation": "^2.13.2",
    "@types/styled-components": "^4.1.0",
    "babel-jest": "^24.5.0",
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.5.0",
    "jest-expo": "^31.0.0",
    "jest-fetch-mock": "^2.1.2",
    "react-dom": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  }
}
